<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LableBarView">
        <attr name="ico" format="reference" />
        <attr name="ico_padding" format="dimension" />
        <attr name="title" format="string" />
        <attr name="title_size" format="dimension" />
        <attr name="title_color" format="color" />
        <attr name="subtitle" format="string" />
        <attr name="subtitle_size" format="dimension" />
        <attr name="subtitle_color" format="color" />
        <attr name="has_arrows" format="boolean"/>

    </declare-styleable>

    <!--************ zomll_Loading ***********-->
    <declare-styleable name="zomll_LoadingView">
        <attr name="zomll_loading_view_size" format="dimension"/>
        <attr name="android:color"/>
    </declare-styleable>

    <declare-styleable name="MLImageView">
        <attr name="ml_border_color" format="color" />
        <attr name="ml_border_width" format="dimension" />
        <attr name="ml_press_alpha" format="integer" />
        <attr name="ml_press_color" format="color" />
        <attr name="ml_radius" format="dimension" />
        <attr name="ml_shape_type" format="enum">
            <enum name="none" value="0" />
            <enum name="round" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
    </declare-styleable>

</resources>